{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "plugin:vue/recommended",
    "eslint:recommended",
    "prettier",
    "prettier/vue"
  ],
  "rules": {
    "vue/require-prop-types": 0,
    "no-console": 0,
    "no-debugger": 0,
    "no-unused-vars": 0,
    "vue/no-v-html": 0
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}
